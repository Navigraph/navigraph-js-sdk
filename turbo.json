{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": { "outputs": [] },
    "dev": { "cache": false },
    "clean": { "cache": false },
    "prepack": { "cache": false },
    "postpack": { "cache": false }
  },
  "globalDependencies": ["tsconfig.json"]
}
