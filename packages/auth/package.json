{
  "name": "@navigraph/auth",
  "version": "2.0.7",
  "license": "MIT",
  "description": "Authentication with Navigraph services as part of the Navigraph SDK",
  "author": "Navigraph <contact@navigraph.com> (https://navigraph.com)",
  "homepage": "https://navigraph.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Navigraph/navigraph-js-sdk",
    "directory": "packages/auth"
  },
  "keywords": [
    "navigraph",
    "charts",
    "AIRAC",
    "fmsdata",
    "sdk",
    "authentication"
  ],
  "main": "./src/index.ts",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --watch --sourcemap inline",
    "lint": "cross-env TIMING=1 eslint src/**/*.ts --fix",
    "clean": "rimraf .turbo node_modules dist",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@navigraph/pkce": "^1.0.3",
    "@navigraph/app": "1.2.6",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/qr-image": "^3.2.5"
  }
}
