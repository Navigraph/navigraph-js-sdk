{
  "name": "@navigraph/weather",
  "version": "1.1.1",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --legacy-output --dts ",
    "dev": "npm-run-all --parallel watch:gql watch:ts",
    "watch:gql": "yarn codegen -w",
    "watch:ts": "tsup src/index.ts --format esm,cjs --legacy-output --watch --dts --sourcemap inline",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.2",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "graphql": "^16.2.0",
    "npm-run-all": "^4.1.5"
  }
}
