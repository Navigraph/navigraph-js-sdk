{
  "name": "navigraph",
  "version": "1.3.0",
  "description": "A library for interacting with Navigraph services",
  "author": "Navigraph <contact@navigraph.com> (https://navigraph.com)",
  "homepage": "https://navigraph.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Navigraph/navigraph-js-sdk"
  },
  "keywords": [
    "navigraph",
    "charts",
    "AIRAC",
