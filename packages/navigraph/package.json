  "dependencies": {
    "@navigraph/app": "1.3.6",
    "@navigraph/auth": "2.5.2",
    "@navigraph/charts": "2.0.5",
    "@navigraph/packages": "1.0.1",
    "@navigraph/amdb": "0.0.2",
    "@navigraph/weather": "1.0.0"
  }
