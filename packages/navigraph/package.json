{
  "name": "navigraph",
  "version": "1.2.0",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --dts ",
    "dev": "tsup src/index.ts --format esm,cjs --legacy-output --watch --dts --sourcemap inline",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rimraf .turbo node_modules dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@navigraph/app": "1.2.0",
    "@navigraph/auth": "1.2.0",
    "@navigraph/charts": "1.2.0"
  }
}
